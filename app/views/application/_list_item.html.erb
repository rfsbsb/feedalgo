<%= render "feed_header", :feed => @feed %>
<div id="feed-container">
  <%= render "feed_entries", :entries => @entries, :feed_users => @feed_users %>
  <% if @entries.next_page %>
    <span data-next-page="2" data-next-page-url="<%= feed_paging_path(CGI.escape(@feed.url)) %>" class="next_page">&nbsp;</span>
  <% else %>
    <div class="no_items">There are no more items</div>
  <% end %>
</div>

<script type="test/javascript">
  attachAccordion();
  update_count('<%= @feed.id %>', '<%= feed_count.zero? ? nil : feed_count %>');
</script>