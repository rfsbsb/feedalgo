<%= render "folder_header", :folder => @folder %>
<div id="feed-container">
  <%= render "feed_entries", :entries => @entries, :feed_users => @folder.feeds %>
  <% if @entries.next_page %>
    <span data-next-page="2" data-next-page-url="<%= folder_paging_path(@folder.id) %>" class="next_page">&nbsp;</span>
  <% else %>
    <div class="no_items">There are no more items</div>
  <% end %>
</div>