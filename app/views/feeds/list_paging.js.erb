$('#reader #feed-container').append('<%= escape_javascript render "feed_entries", :entries => @entries, :feed_users => @feed_users %>');
<% if @entries.next_page %>
  $('#reader #feed-container').append('<span data-next-page="<%= @entries.next_page %>" data-next-page-url="<%= feed_paging_path(CGI.escape(@feed.url)) %>" class="next_page">&nbsp;</span>');
<% else %>
  $('#reader #feed-container').append('<div class="no_items">There are no more items</div>');
<% end %>
attachAccordion();