<div class="header">
  <h2>
    <span id="title" data-resource="feed_user" data-type="text" data-url="/feed_users/<%= feed.id %>" data-title="Enter feed name"><%= feed.title %></span>
    <small class="edit-title">Edit</small>
  </h2>

  <span data-unread-url="<%= unread_path(CGI.escape(feed.url))%>" class="unread_url"></span>
  <span data-read-url="<%= reader_feed_path(CGI.escape(feed.url))%>" class="read_url"></span>
  <span data-mark-all-url="<%= mark_all_path(CGI.escape(feed.url))%>" class="mark_all_url"></span>
  <%= render "header_buttons" %>
</div>